RewriteEngine On

RewriteCond %{HTTPS} off
RewriteCond %{HTTP:X-Forwarded-SSL} !on
RewriteCond %{HTTP_HOST} ^nyasajob\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.nyasajob\.com$
RewriteRule ^(.*)$ "https\:\/\/nyasajob\.com\/$1" [R=301,L]

RewriteCond %{HTTP_USER_AGENT} ^.*(AwarioSmartBot).*$
RewriteRule .* - [F,L]

RewriteEngine On
RewriteCond %{HTTP_USER_AGENT} ^.*(facebookexternalhit).*$
RewriteRule .* - [F,L]


<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteCond %{REQUEST_URI} !^public
    RewriteRule ^(.*)$ public/$1 [L]
</IfModule>

